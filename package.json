    "tsc-build": "tsc",
    "test": "jest --passWithNoTests --colors",
	"docker:test": "yarn test --runInBand --detectOpenHandles",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "prepare": "husky install"
